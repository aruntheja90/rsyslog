# Transport system and kernal (syslog) messages to logstash
*.*;syslog;auth,authpriv.none action(
	type="omfwd"
	Target="{{ rsyslog.logstash.host }}"
	Port="{{ rsyslog.logstash.inputs.json.port }}"
	Protocol="tcp"
	RebindInterval="100"
	template="syslogTemplate"
)
stop
